{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@components": [
        "src/app/components/public-api.ts"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@models": [
        "src/app/models/public-api.ts"
      ],
      "@models/*": [
        "src/app/models/*"
      ],
      "@service": [
        "src/app/service/public-api.ts"
      ],
      "@service/*": [
        "src/app/service/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
